{
    "rewrites": [
        {
            "source": "/((?!assets|favicon|og-image|apple-touch-icon|web-app-manifest|placeholder|sitemap|robots|googleda422e9133accc04|site.webmanifest|timeline).*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/sitemap.xml",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/xml"
                }
            ]
        },
        {
            "source": "/robots.txt",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/plain"
                }
            ]
        }
    ]
}